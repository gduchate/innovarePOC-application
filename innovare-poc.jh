
application {
  config {
    baseName myApp1,
    applicationType microservice,
    packageName com.myapp,
    authenticationType jwt,
    prodDatabaseType mysql,
  }
  entities Book, Publisher
}



entity Book {
	name String,
    color String,
    price Long,
}

entity Publisher {
	name String,
    address String
}



relationship ManyToOne {
  Book to Publisher
}
    